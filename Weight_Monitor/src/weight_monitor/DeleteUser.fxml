<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #bbafaf;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="weight_monitor.DeleteUserController">
   <children>
      <Label layoutX="33.0" layoutY="65.0" text="Are you sure you want to delete this user?" textFill="#1e1d1d">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="no_delete" layoutX="219.0" layoutY="113.0" mnemonicParsing="false" onAction="#no" prefHeight="20.0" prefWidth="70.0" text="No">
         <effect>
            <InnerShadow color="#9e9191" />
         </effect>
      </Button>
      <Button fx:id="ok_delete" layoutX="124.0" layoutY="113.0" mnemonicParsing="false" onAction="#ok" prefHeight="20.0" prefWidth="70.0" text="Yes">
         <effect>
            <InnerShadow color="#9e9191" />
         </effect>
      </Button>
   </children>
</AnchorPane>
